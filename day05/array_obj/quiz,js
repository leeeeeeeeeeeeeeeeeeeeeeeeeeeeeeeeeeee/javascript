const a1=data.filter((x)=>x.gender =="male" && (x.dapartment == "Accounting" || x.dapartment == "Sales"))

// 2. Ïù¥Î©îÏùº Ï£ºÏÜåÍ∞Ä .com ÎÅùÎÇòÍ≥† ÏÑ±Î≥ÑÏù¥ Ïó¨ÏÑ±Ïù∏ ÏÇ¨ÎûåÎì§Îßå ÏΩòÏÜîÎ°ú ÎÇòÌÉÄÎÇ¥Í∏∞!
data,filter((x)=> x.gender == "famale" && x.email_address.includes(".com"));

// 3. ÌÅ¨Î†àÎîß Ïπ¥ÎìúÎ•º Îí∑ÏûêÎ¶¨ 4Í∞úÎ•º ÎÇ®Í∏∞Í≥† ÎÇòÎ®∏ÏßÄÎäî *Î°ú Î∞îÍæ∏Í≥†, Ïù¥Î©îÏùº Ïïû Î∂ÄÎ∂ÑÎßå ÎÇ®Í∏∞Í≥† 
// Î∂ÄÏÑúÍ∞Ä ÏÑúÎπÑÏä§Ïù∏ ÏÇ¨ÎûåÎì§Îßå ÏΩòÏÜîÎ°ú ÎÇòÌÉÄÎÇ¥Í∏∞!
data.filter((x)=> x.dapartment == "services").map((x)=>{
    x.credit_card = x.credit_card.split("").map((x,i)=> i < 12 ? "*" : x).reduce((a,c)=> a+c);
    x.email_address = x.email_address.split("@")[0];
    return x;
});